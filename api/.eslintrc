{
  // "extends": "iinitz/ts-base",
  // "parserOptions": {
  //   "project": "./tsconfig.json"
  // },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "linebreak-style": ["error", "windows"],
    "global-require": 0,
    "eslint linebreak-style": [0, "error", "windows"]
  }
}
