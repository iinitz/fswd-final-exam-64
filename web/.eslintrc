{
  "extends": "iinitz/ts-react",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": 0,
    "max-len": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "@typescript-eslint/no-misused-promises": 0
  }
}
/*eslint linebreak-style: ["error", "windows"]*/